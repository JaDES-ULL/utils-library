@prefix : <http://example.org/sample#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .

<http://example.org/sample-data>
    a owl:Ontology ;
    dc:description "Ontology designed to fail when loaded, since it does not import the main ontology"@en .

:BiotinidaseDeficiency_General a owl:NamedIndividual, :Disease ;
    :hasDescription "Deficiencia de Biotinidasa (DB)" ;
    :includedByModel :BiotinidaseDeficiency_Model ;
                rdfs:label "Biotinidase deficiency"@en ,
                           "Deficiencia de biotinidasa"@es .

:BiotinidaseDeficiency_Model a owl:NamedIndividual, :SimulationModel ;
                rdfs:label "Model for biotinidase deficiency"@en ,
                           "Modelo para la deficiencia de biotinidasa"@es .
